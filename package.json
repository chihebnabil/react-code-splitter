{
  "name": "react-code-splitter",
  "version": "2.0.1",
  "description": "Interactive jscodeshift transform for splitting React component files into subcomponents",
  "main": "split-components.js",
  "bin": {
    "react-split": "./bin/cli.js",
    "react-code-splitter": "./bin/cli.js"
  },
  "scripts": {
    "test": "jest",
    "transform": "jscodeshift -t split-components.js",
    "transform:dry": "jscodeshift -t split-components.js --dry",
    "prepublishOnly": "npm test",
    "version": "npm test"
  },
  "keywords": [
    "jscodeshift",
    "react",
    "codemod",
    "refactoring",
    "component-splitting",
    "code-splitting",
    "component-extraction",
    "react-refactor",
    "cli"
  ],
  "author": "Chiheb Nabil",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chihebnabil/react-code-splitter.git"
  },
  "bugs": {
    "url": "https://github.com/chihebnabil/react-code-splitter/issues"
  },
  "homepage": "https://nextlovable.com/nextjs-experts",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "split-components.js",
    "split-components-interactive.js",
    "split-by-selector.js",
    "utils.js",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "glob": "^7.2.3",
    "inquirer": "^8.2.5",
    "jscodeshift": "^0.15.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "jest": "^29.0.0"
  }
}
